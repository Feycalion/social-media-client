{
  "name": "css-frameworks-ca",
  "version": "1.0.4",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier -w src/**/*.js",
    "test": "npm run test-unit && npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test-unit": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "eslint --cache --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@eslint/js": "^9.0.0",
    "babel-loader": "^9.1.3",
    "cypress": "^13.7.3",
    "eslint": "^9.0.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-jest": "^28.6.0",
    "globals": "^15.0.0",
    "husky": "^9.1.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "live-server": "^1.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3",
    "jest-localstorage-mock": "^2.4.26"
  }
}
