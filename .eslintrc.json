{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [
    {
      "files": [
        "**/*.cy.js",
        "src/js/api/auth/login.test.js",
        "./cypress/support/commands.js",
        "cypress.config.js"
      ],

      "env": { "cypress/globals": true, "jest/globals": true },
      "plugins": ["cypress", "jest"],
      "extends": ["plugin:cypress/recommended"],
      "rules": {
        "cypress/no-unnecessary-waiting": "off",
        "no-unused-vars": "off"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },

  "rules": {}
}
